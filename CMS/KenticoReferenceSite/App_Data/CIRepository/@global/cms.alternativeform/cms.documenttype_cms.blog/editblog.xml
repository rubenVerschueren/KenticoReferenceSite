          <width>500</width>
          <dialogs_libraries_site>##all##</dialogs_libraries_site>
          <dialogs_content_site>##all##</dialogs_content_site>
          <toolbarset>Basic</toolbarset>
        </settings>
      </field>
      <field column="BlogSideColumnText">
        <settings>
          <width>500</width>
          <toolbarset>Basic</toolbarset>
        </settings>
      </field>
      <field column="BlogSendCommentsToEmail" inputcontrolstyle="width:270px !important" />
    </form>
  </FormDefinition>
  <FormDisplayName>Edit blog</FormDisplayName>
  <FormGUID>2ff7fcce-df89-4071-a133-8c5b9570d41c</FormGUID>
  <FormHideNewParentFields>False</FormHideNewParentFields>
  <FormLayout>
<![CDATA[
<table>
    <tbody>
        <tr>
            <td>$$label:BlogName$$</td>
            <td>$$input:BlogName$$$$validation:BlogName$$</td>
        </tr>
        <tr>
            <td>$$label:BlogDescription$$</td>
            <td>$$input:BlogDescription$$$$validation:BlogDescription$$</td>
        </tr>
        <tr>
            <td>$$label:BlogSideColumnText$$</td>
            <td>$$input:BlogSideColumnText$$$$validation:BlogSideColumnText$$</td>
        </tr>
        <tr>
            <td>$$label:BlogTeaser$$</td>
            <td>$$input:BlogTeaser$$$$validation:BlogTeaser$$</td>
        </tr>
        <tr>
            <td>$$label:BlogSendCommentsToEmail$$</td>
            <td>$$input:BlogSendCommentsToEmail$$$$validation:BlogSendCommentsToEmail$$</td>
        </tr>
    </tbody>
</table>
]]>
  </FormLayout>
  <FormName>EditBlog</FormName>
</cms.alternativeform>